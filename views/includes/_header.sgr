nav
  #logo
    a(href='/')
      img(src='/img/ts-logo-lockup-color.png')

  #nav-toggle
    input(type='checkbox')
    span
    span
    span

  ul#nav-panel
    each(loop='item in ["index", "who we are"]')
      if(condition='item === "index"')
        li: a(href='/') home
      elseif(condition='item === "who we are"')
        li: a(href='/about') who we are
      else
        li: a(href='/{{ item }}') {{ item }}

